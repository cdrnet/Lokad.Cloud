<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Lokad.Cloud" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WebRole name="LokadCloudWebRole">
    <InputEndpoints>
      <!-- Must use port 80 for http and port 443 for https when running in the cloud -->
      <InputEndpoint name="HttpIn" protocol="http" port="80" />
    </InputEndpoints>
  </WebRole>
  <WorkerRole name="LokadCloudWorkerRole">
		<ConfigurationSettings>
			<Setting name="AccountName" />
			<Setting name="AccountKey" />
			<Setting name="BlobEndpoint" />
			<Setting name="QueueEndpoint" />
		</ConfigurationSettings>
  </WorkerRole>
</ServiceDefinition>
